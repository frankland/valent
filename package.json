{
  "name": "valent",
  "version": "0.1.0-rc20",
  "repository": "https://github.com/frankland/valent",
  "description": "Helpfull ES6 runtime for angular",
  "scripts": {
    "test": "mocha test/index.js --compilers js:babel-core/register",
    "prepublish": "./node_modules/.bin/babel --stage=0 -d ./ ./src",
    "dev": "./node_modules/.bin/babel -d ./ ./src --watch",
    "format": "prettier --trailing-comma es5 --single-quote --write './src/**/*.js' './test/**/*.js'",
    "precommit": "lint-staged"
  },
  "main": "./valent.js",
  "keywords": [
    "angular",
    "es6",
    "controller",
    "directive",
    "magic"
  ],
  "license": "MIT",
  "dependencies": {
    "deep-diff": "^0.3.8",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "tcomb": "^3.2.20",
    "tcomb-validation": "^3.3.0",
    "url-pattern": "^1.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.0.2",
    "expect.js": "^0.3.1",
    "husky": "^0.13.4",
    "lint-staged": "^3.6.1",
    "mocha": "^3.4.2",
    "prettier": "^1.4.4",
    "rimraf": "^2.6.1"
  },
  "lint-staged": {
    "*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  }
}
